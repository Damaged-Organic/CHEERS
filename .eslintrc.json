{
    "extends": "eslint:recommended",
    "env": {
        "node": true,
        "es6": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 8,
        "sourceType": "module",
        "impliedStrict": true
    },
    "rules": {
        "strict": 0,
        "no-console": 0
    }
}
